{
	"model": "openai/gpt-oss-120b",
	"provider": "fireworks-ai",
	"inputs": [
		{
			"type": "promptString",
			"id": "hf-token",
			"description": "Your Hugging Face Token",
			"password": true
		}
	],
	"servers": [
		{
			"type": "http",
			"url": "https://huggingface.co/mcp?bouquet=docs",
			"headers":
			{
				"Authorization": "Bearer ${input:hf-token}"
			}
		}
	]
}
